{
    "name": "My Dev iamgedetection Container",
    "build": {
        "dockerfile": "../DevDokcerfile",
        "context": ".."
    },
    "mounts": [
        "source=${localWorkspaceFolder}/data,target=/CICProject/data,type=bind"
    ],
    "workspaceFolder": "/CICProject",
    "customizations": {
        "vscode": {
            "settings": {
                "terminal.integrated.shell.linux": "/bin/bash"
            },
            "extensions": [
                "ms-python.python",
                "ms-azuretools.vscode-docker"
            ]
        }
    }
}
